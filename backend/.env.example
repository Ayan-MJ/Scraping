# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=scraping-wizard-api

# Environment - options: development, test, production
ENVIRONMENT=development

# Database Configuration
# Set to true to use in-memory database for testing
USE_INMEM_DB=false

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Redis Configuration (for Celery)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_URL=redis://redis:6379/0

# Celery Configuration
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# CORS Configuration
# Use ["*"] for development, specify exact domains in production
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000","https://your-domain.com"]

# Sentry Configuration (Error Monitoring)
# Leave empty to disable Sentry
SENTRY_DSN=

# Server Identification (Optional)
VERSION=0.1.0
SERVER_NAME=scraping-wizard-api-local

# Authentication Testing (for e2e tests)
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=test_password 